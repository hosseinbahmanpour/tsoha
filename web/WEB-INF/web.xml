<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>YhteysTestaaja</servlet-name>
        <servlet-class>Taksidb.Tietokanta.YhteysTestaaja</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Yhteys</servlet-name>
        <servlet-class>Tietokanta.YhteysTesti</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Testi</servlet-name>
        <servlet-class>Tietokanta.Testi</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Listaustesti</servlet-name>
        <servlet-class>Testit.ListausTesti</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>KayttajaServlet</servlet-name>
        <servlet-class>Servletit.KayttajaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AutoServlet</servlet-name>
        <servlet-class>Servletit.AutoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AjovuoroServlet</servlet-name>
        <servlet-class>Servletit.AjovuoroServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>KuljettajaServlet</servlet-name>
        <servlet-class>Servletit.KuljettajaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>KyytiServlet</servlet-name>
        <servlet-class>Servletit.KyytiServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>YhteysTestaaja</servlet-name>
        <url-pattern>/YhteysTestaaja</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Yhteys</servlet-name>
        <url-pattern>/Yhteys</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Testi</servlet-name>
        <url-pattern>/Testi</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Listaustesti</servlet-name>
        <url-pattern>/Listaustesti</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>KayttajaServlet</servlet-name>
        <url-pattern>/KayttajaServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AutoServlet</servlet-name>
        <url-pattern>/AutoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AjovuoroServlet</servlet-name>
        <url-pattern>/AjovuoroServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>KuljettajaServlet</servlet-name>
        <url-pattern>/KuljettajaServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>KyytiServlet</servlet-name>
        <url-pattern>/KyytiServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
